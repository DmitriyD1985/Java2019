public class Zadanie2 {
    public static void main (String args[]) {

    int[] myArr = {1, 2, 2000, 4, 1001, 6, 7,8};
        int sum=0;
        for(int i=0; i<myArr.length; i++) {
            sum=sum+myArr[i];
        }
        System.out.println(sum);

        for(int i=0; i<myArr.length; i++) {
            if(myArr[i]>1000) {
                System.out.println ("Массив массивный");
            break;}

            }
        }

        }
